{
  "name": "sissi-snaps",
  "version": "1.2.0",
  "description": "Taking snapshots for sissi (simple static sites)",
  "author": "Alex Kott",
  "license": "ISC",
  "repository": "github.com/AlexKott/sissi-snaps",
  "scripts": {
    "build": "babel --out-dir lib src",
    "build:watch": "yarn build --watch",
    "prepare": "rm -rf lib/* && yarn build"
  },
  "bin": {
    "sissi-snaps": "./bin/sissi-snaps.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "express": "^4.16.2",
    "jsdom": "^11.3.0",
    "mkdirp": "^0.5.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "8"
          }
        }
      ]
    ]
  },
  "keywords": [
    "sissi",
    "react",
    "snapshot",
    "static-site-generator"
  ]
}
