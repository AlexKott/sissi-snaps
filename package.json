{
  "name": "sissi-snaps",
  "version": "1.3.1",
  "description": "Taking snapshots for sissi (simple static sites)",
  "author": "Alex Kott",
  "license": "ISC",
  "repository": "github.com/AlexKott/sissi-snaps",
  "scripts": {
    "build": "babel --out-dir lib src",
    "prepare": "rm -rf lib/* && npm run build"
  },
  "bin": {
    "sissi-snaps": "./bin/sissi-snaps.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "express": "^4.16.3",
    "jsdom": "^11.11.0",
    "mkdirp": "^0.5.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "8"
          }
        }
      ]
    ]
  },
  "keywords": [
    "sissi",
    "react",
    "snapshot",
    "static-site-generator"
  ]
}
