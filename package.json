{
  "name": "sissi-snaps",
  "version": "1.0.0",
  "description": "Taking snapshots for sissi (simple static sites)",
  "author": "Alex Kott",
  "license": "ISC",
  "repository": "github.com/AlexKott/sissi-snaps",
  "scripts": {
    "build": "babel --out-dir lib src",
    "build:watch": "yarn build --watch",
    "prepare": "rm -rf lib/* && yarn build"
  },
  "bin": {
    "sissi-snaps": "./bin/sissi-snaps.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.13.2",
    "express": "^4.16.2",
    "jsdom": "^11.3.0",
    "mkdirp": "^0.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "sissi",
    "react",
    "snapshot",
    "static-site-generator"
  ]
}
